<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BbsDetailActivity">

    <TextView
            android:text="내용: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView12"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintVertical_bias="0.249"/>
    <TextView
            android:layout_width="469dp"
            android:layout_height="26dp" android:id="@+id/ddateText"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="@+id/textView10"/>
    <TextView
            android:text="날짜: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView10"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintVertical_bias="0.202"/>
    <TextView
            android:layout_width="444dp"
            android:layout_height="26dp" android:id="@+id/dreadcntText"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.362" app:layout_constraintTop_toTopOf="@+id/textView9"/>
    <TextView
            android:text="조회수 : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintVertical_bias="0.158"/>
    <TextView
            android:layout_width="444dp"
            android:layout_height="26dp" android:id="@+id/dwriterText"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.362" app:layout_constraintTop_toTopOf="@+id/textView7"/>
    <TextView
            android:text="작성자 : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintVertical_bias="0.115"/>
    <TextView
            android:text="제목: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:textSize="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintVertical_bias="0.077"/>
    <TextView
            android:layout_width="469dp"
            android:layout_height="26dp" android:id="@+id/dtitleText"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="@+id/textView5"/>
    <TextView
            android:layout_width="529dp"
            android:layout_height="527dp" android:id="@+id/dcontentText"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.661" app:layout_constraintTop_toBottomOf="@+id/textView12"
            android:layout_marginTop="8dp"/>
    <Button
            android:text="목록으로"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/dlistBtn"
            app:layout_constraintTop_toBottomOf="@+id/dcontentText"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.179"/>
    <Button
            android:text="삭제"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ddelBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toEndOf="@+id/dupdateBtn"
            app:layout_constraintBottom_toTopOf="@+id/dcontentText"
            app:layout_constraintTop_toBottomOf="@+id/ddateText" app:layout_constraintHorizontal_bias="0.17"
            app:layout_constraintVertical_bias="0.4"/>
    <Button
            android:text="수정"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/dupdateBtn"
            app:layout_constraintStart_toEndOf="@+id/textView12" android:layout_marginStart="280dp"
            app:layout_constraintTop_toBottomOf="@+id/ddateText"
            app:layout_constraintBottom_toTopOf="@+id/dcontentText" app:layout_constraintVertical_bias="0.2"/>
</androidx.constraintlayout.widget.ConstraintLayout>