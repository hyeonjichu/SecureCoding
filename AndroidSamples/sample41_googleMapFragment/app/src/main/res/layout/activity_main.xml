<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <FrameLayout
            android:id="@+id/content"
            android:layout_width="match_parent"
            android:layout_height="602dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/longEdit"
            app:layout_constraintVertical_bias="0.638">

    </FrameLayout>
    <TextView
            android:text="주소: "
            android:textSize="30dp"
            android:layout_width="87dp"
            android:layout_height="40dp"
            android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="52dp"
            app:layout_constraintHorizontal_bias="0.087"/>
    <EditText
            android:layout_width="417dp"
            android:layout_height="56dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/addrEdit"
            app:layout_constraintStart_toEndOf="@+id/textView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintHorizontal_bias="0.164"/>
    <EditText
            android:layout_width="167dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/latEdit"
            app:layout_constraintTop_toBottomOf="@+id/addrEdit"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="36dp"
            android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="175dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/longEdit"
            app:layout_constraintTop_toBottomOf="@+id/addrEdit"
            android:layout_marginTop="36dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="68dp"
            app:layout_constraintStart_toEndOf="@+id/latEdit"
            app:layout_constraintHorizontal_bias="0.191"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            app:layout_constraintTop_toBottomOf="@+id/addrEdit"
            app:layout_constraintStart_toEndOf="@+id/longEdit" android:layout_marginTop="36dp"
            android:layout_marginStart="20dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>