<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="196dp"
            android:layout_height="60dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editUpSeq"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.705" app:layout_constraintHorizontal_bias="0.126"/>
    <Button
            android:text="업데이트"
            android:layout_width="246dp"
            android:layout_height="60dp" android:id="@+id/updateBtn"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.788"/>
    <EditText
            android:layout_width="498dp"
            android:layout_height="64dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editDel"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.27"/>
    <Button
            android:text="삭제"
            android:layout_width="246dp"
            android:layout_height="60dp" android:id="@+id/delBtn"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.277"/>
    <EditText
            android:layout_width="498dp"
            android:layout_height="64dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editInsert"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.068"/>
    <EditText
            android:layout_width="498dp"
            android:layout_height="64dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editSelect"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.478"/>
    <Button
            android:text="검색"
            android:layout_width="246dp"
            android:layout_height="60dp" android:id="@+id/selectBtn"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.518"/>
    <Button
            android:text="추가"
            android:layout_width="246dp"
            android:layout_height="60dp" android:id="@+id/insert"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.033"/>
    <EditText
            android:layout_width="256dp"
            android:layout_height="60dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editUpTxt"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toEndOf="@+id/editUpSeq"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.474" app:layout_constraintVertical_bias="0.709"/>
</androidx.constraintlayout.widget.ConstraintLayout>